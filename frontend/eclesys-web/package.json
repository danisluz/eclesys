{
  "name": "eclesys-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve -o --proxy-config proxy.local.json",
    "start": "ng serve -o --proxy-config proxy.local.json",
    "start:hmr": "ng serve --hmr -o --proxy-config proxy.local.json",
    "dev:docker": "ng serve --host 0.0.0.0 --port 4200 --proxy-config proxy.docker.json",
    "build": "ng build",
    "build:dev": "ng build --configuration development",
    "build:prod": "ng build --configuration production",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "test": "vitest",
    "test:watch": "vitest --watch"
  },
  "private": true,
  "packageManager": "npm@11.6.2",
  "dependencies": {
    "@angular/animations": "^21.0.8",
    "@angular/cdk": "^21.0.6",
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/material": "^21.0.6",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-server": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@angular/ssr": "^21.0.5",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.5",
    "@angular/cli": "^21.0.5",
    "@angular/compiler-cli": "^21.0.0",
    "@types/express": "^5.0.1",
    "autoprefixer": "^10.4.23",
    "jsdom": "^27.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
